{"ast":null,"code":"import PokemonsPaginations from \"@/components/PokemonsPaginations\";\nexport default {\n  name: \"FindPokemon\",\n  components: {\n    PokemonsPaginations\n  },\n\n  data() {\n    return {\n      placeholderString: 'Найди своего покемона',\n      inputValue: '',\n      pokemonName: '',\n      pokemonWeight: '',\n      pokemonImg: '',\n      pokemonHeight: '',\n      pokemonId: '',\n      pokemonStats: '',\n      pokemonType: ''\n    };\n  },\n\n  methods: {\n    async getOnePokemonData(query) {\n      let url = `https://pokeapi.co/api/v2/pokemon/${query}/`;\n      let response = await fetch(url);\n      let pokemon = await response.json();\n      return pokemon;\n    },\n\n    async showPokemon(namePokemon) {\n      let character = await this.getOnePokemonData(namePokemon);\n      this.pokemonName = character.name;\n      this.pokemonWeight = character.weight;\n      this.pokemonHeight = character.height;\n      this.pokemonId = character.id;\n      this.pokemonStats = character.stats;\n      this.pokemonType = character.types;\n      this.pokemonImg = character.sprites.other.dream_world.front_default;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,mBAAP,MAAgC,kCAAhC;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAC;IACTF;EADS,CAFE;;EAKbG,IAAI,GAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,uBADd;MAELC,UAAU,EAAE,EAFP;MAGLC,WAAW,EAAE,EAHR;MAILC,aAAa,EAAE,EAJV;MAKLC,UAAU,EAAE,EALP;MAMLC,aAAa,EAAE,EANV;MAOLC,SAAS,EAAE,EAPN;MAQLC,YAAY,EAAE,EART;MASLC,WAAW,EAAE;IATR,CAAP;EAYD,CAlBY;;EAmBbC,OAAO,EAAE;IACP,MAAMC,iBAAN,CAAwBC,KAAxB,EAA+B;MAC7B,IAAIC,GAAE,GAAK,qCAAoCD,KAAM,GAArD;MACA,IAAIE,QAAO,GAAI,MAAMC,KAAK,CAACF,GAAD,CAA1B;MACA,IAAIG,OAAM,GAAI,MAAMF,QAAQ,CAACG,IAAT,EAApB;MACA,OAAOD,OAAP;IACD,CANM;;IAOP,MAAME,WAAN,CAAkBC,WAAlB,EAA+B;MAC7B,IAAIC,SAAQ,GAAI,MAAM,KAAKT,iBAAL,CAAuBQ,WAAvB,CAAtB;MACA,KAAKhB,WAAL,GAAoBiB,SAAS,CAACtB,IAA9B;MACA,KAAKM,aAAL,GAAsBgB,SAAS,CAACC,MAAhC;MACA,KAAKf,aAAL,GAAsBc,SAAS,CAACE,MAAhC;MACA,KAAKf,SAAL,GAAkBa,SAAS,CAACG,EAA5B;MACA,KAAKf,YAAL,GAAqBY,SAAS,CAACI,KAA/B;MACA,KAAKf,WAAL,GAAoBW,SAAS,CAACK,KAA9B;MACA,KAAKpB,UAAL,GAAkBe,SAAS,CAACM,OAAV,CAAkBC,KAAlB,CAAwBC,WAAxB,CAAoCC,aAAtD;IACF;;EAhBO;AAnBI,CAAf","names":["PokemonsPaginations","name","components","data","placeholderString","inputValue","pokemonName","pokemonWeight","pokemonImg","pokemonHeight","pokemonId","pokemonStats","pokemonType","methods","getOnePokemonData","query","url","response","fetch","pokemon","json","showPokemon","namePokemon","character","weight","height","id","stats","types","sprites","other","dream_world","front_default"],"sourceRoot":"","sources":["D:\\zadaniya\\pokemons\\src\\components\\FindPokemon.vue"],"sourcesContent":["<template>\r\n  <div class=\"FindPokemon\">\r\n\r\n    <div class=\"pokemon\" v-if=\"pokemonName\">\r\n      <img :src=\"pokemonImg\" alt=\"Фото покемона грузится\">\r\n      <h1> Name: {{ pokemonName }}</h1>\r\n      <h2> Hewiht: {{ pokemonWeight }} кг</h2>\r\n      <h2> Height: {{ pokemonHeight }} </h2>\r\n      <ul> <h2>Stats:</h2>\r\n        <li v-for=\"stat in pokemonStats\" :key=\"stat.id\">\r\n          {{ stat.stat.name }}: {{ stat.base_stat }}\r\n        </li>\r\n      </ul>\r\n      <ul> <h2>Types:</h2>\r\n        <li v-for=\"type in pokemonType\" :key=\"type.id\">\r\n          {{ type.type.name }}\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <input type=\"text\"\r\n           :placeholder=\"placeholderString\"\r\n           v-model=\"inputValue\"\r\n           @keypress.enter=\"showPokemon(inputValue)\">\r\n  </div>\r\n  <PokemonsPaginations\r\n      v-on:show-pokemon=\"showPokemon\"\r\n  />\r\n</template>\r\n\r\n<script>\r\n\r\nimport PokemonsPaginations from \"@/components/PokemonsPaginations\";\r\n\r\nexport default {\r\n  name: \"FindPokemon\",\r\n  components:{\r\n    PokemonsPaginations\r\n  },\r\n  data() {\r\n    return {\r\n      placeholderString: 'Найди своего покемона',\r\n      inputValue: '',\r\n      pokemonName: '',\r\n      pokemonWeight: '',\r\n      pokemonImg: '',\r\n      pokemonHeight: '',\r\n      pokemonId: '',\r\n      pokemonStats: '',\r\n      pokemonType: '',\r\n\r\n    }\r\n  },\r\n  methods: {\r\n    async getOnePokemonData(query) {\r\n      let url = `https://pokeapi.co/api/v2/pokemon/${query}/`\r\n      let response = await fetch(url)\r\n      let pokemon = await response.json()\r\n      return pokemon\r\n    },\r\n    async showPokemon(namePokemon) {\r\n      let character = await this.getOnePokemonData(namePokemon)\r\n      this.pokemonName =  character.name\r\n      this.pokemonWeight =  character.weight\r\n      this.pokemonHeight =  character.height\r\n      this.pokemonId =  character.id\r\n      this.pokemonStats =  character.stats\r\n      this.pokemonType =  character.types\r\n      this.pokemonImg = character.sprites.other.dream_world.front_default\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}