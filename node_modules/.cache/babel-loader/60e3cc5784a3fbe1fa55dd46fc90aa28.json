{"ast":null,"code":"import PokemonsPaginations from \"@/components/PokemonsPaginations\";\nexport default {\n  name: \"FindPokemon\",\n  components: {\n    PokemonsPaginations\n  },\n\n  data() {\n    return {\n      placeholderString: 'Найди своего покемона',\n      inputValue: '',\n      pokemonName: '',\n      pokemonWeight: '',\n      pokemonImg: '',\n      pokemonHeight: '',\n      pokemonId: '',\n      pokemonHp: ''\n    };\n  },\n\n  methods: {\n    async getOnePokemonData(query) {\n      let url = `https://pokeapi.co/api/v2/pokemon/${query}/`;\n      let response = await fetch(url);\n      let pokemon = await response.json();\n      return pokemon;\n    },\n\n    async showPokemon(namePokemon) {\n      let character = await this.getOnePokemonData(namePokemon);\n      this.pokemonName = character.name;\n      this.pokemonWeight = character.weight;\n      this.pokemonHeight = character.height;\n      this.pokemonId = character.order;\n      this.pokemonHp = character.stats[0].base_stat;\n      this.pokemonImg = character.sprites.other.dream_world.front_default;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuBA,OAAOA,mBAAP,MAAgC,kCAAhC;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAC;IACTF;EADS,CAFE;;EAKbG,IAAI,GAAG;IACL,OAAO;MACLC,iBAAiB,EAAE,uBADd;MAELC,UAAU,EAAE,EAFP;MAGLC,WAAW,EAAE,EAHR;MAILC,aAAa,EAAE,EAJV;MAKLC,UAAU,EAAE,EALP;MAMLC,aAAa,EAAE,EANV;MAOLC,SAAS,EAAE,EAPN;MAQLC,SAAS,EAAE;IARN,CAAP;EAUD,CAhBY;;EAiBbC,OAAO,EAAE;IACP,MAAMC,iBAAN,CAAwBC,KAAxB,EAA+B;MAC7B,IAAIC,GAAE,GAAK,qCAAoCD,KAAM,GAArD;MACA,IAAIE,QAAO,GAAI,MAAMC,KAAK,CAACF,GAAD,CAA1B;MACA,IAAIG,OAAM,GAAI,MAAMF,QAAQ,CAACG,IAAT,EAApB;MACA,OAAOD,OAAP;IACD,CANM;;IAOP,MAAME,WAAN,CAAkBC,WAAlB,EAA+B;MAC7B,IAAIC,SAAQ,GAAI,MAAM,KAAKT,iBAAL,CAAuBQ,WAAvB,CAAtB;MACA,KAAKf,WAAL,GAAoBgB,SAAS,CAACrB,IAA9B;MACA,KAAKM,aAAL,GAAsBe,SAAS,CAACC,MAAhC;MACA,KAAKd,aAAL,GAAsBa,SAAS,CAACE,MAAhC;MACA,KAAKd,SAAL,GAAkBY,SAAS,CAACG,KAA5B;MACA,KAAKd,SAAL,GAAkBW,SAAS,CAACI,KAAV,CAAgB,CAAhB,EAAmBC,SAArC;MACA,KAAKnB,UAAL,GA